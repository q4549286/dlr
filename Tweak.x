                } else if (trimmedLine.length > 0) {
                    NSArray *items = [trimmedLine componentsSeparatedByString:@"|"];
                    NSMutableString *rowString = [NSMutableString string];
                    NSInteger lineCharCount = 0;
                    for (int i = 0; i < items.count; ++i) {
